/*! Import by _C[20], whose real path we cannot found. */
.Hotel {
    width: 100%;
    height: 888rpx;
    background: #fff;
    position: fixed;
    bottom: 0;
    z-index: 201;
}

.Hotel .Hoteltitle {
    width: 100%;
    height: 94rpx;
    position: relative;
}

.Hotel .Hoteltitle .L {
    width: 426rpx;
    height: 94rpx;
    padding: 20rpx 0;
    margin: 0 auto;
    text-align: center;
}

.Hotel .Hoteltitle .L text {
    width: 426rpx;
}

.Hotel .Hoteltitle .L text:first-child {
    color: #333;
    font-size: 16px;
    font-weight: 700;
}

.Hotel .Hoteltitle .close {
    position: absolute;
    right: 14rpx;
    color: #aaa;
    font-size: 30px;
    top: 0;
}

.Prompt {
    width: 100%;
    height: 100rpx;
    line-height: 100rpx;
    background: #ffecdb;
    color: #ff957e;
}

.hotelweek {
    width: 100%;
    height: 120rpx;
    background: #fafafa;
}

.hotelweek .week {
    float: left;
    width: 100rpx;
    height: 120rpx;
    text-align: center;
    line-height: 120rpx;
    color: #aaa;
    font-size: 14px;
    margin-left: 7rpx;
}

.Hotel .month {
    width: 100%;
    height: 600rpx;
    overflow-y: auto;
}

.Hotel .month .monthday {
    float: left;
    width: 105rpx;
    height: 80rpx;
    text-align: center;
    color: #aaa;
    font-size: 14px;
    margin-top: 20rpx;
}

.Hotel .month .monthday view {
    height: 80rpx;
}

.Hotel .month .monthday .ymd {
    width: 100%;
    height: 40rpx;
    text-align: center;
    line-height: 40rpx;
    font-size: 14px;
    clear: both;
    color: #aaa;
}

.Hotel .month .monone {
    width: 98%;
    height: 570rpx;
    margin: 50rpx auto;
}

.Hotel .month .monone .ymd {
    width: 100%;
    height: 40rpx;
    text-align: center;
    line-height: 40rpx;
    color: #333;
    font-size: 14px;
    clear: both;
    padding-bottom: 20rpx;
}
/*! Import end */
.Numberdays {
    width: 100rpx;
    height: 60rpx;
    line-height: 60rpx;
    letter-spacing: 2rpx;
}

.active-tag {
    color: #fff!important;
    background: #36bb81!important;
}

.active-tag2 {
    background: #d7f1e6!important;
    color: #36bb81;
}

.search {
    height: 100rpx;
    line-height: 100rpx;
}

.search_button {
    height: 80rpx;
    margin: 20rpx auto;
    border-radius: 10rpx;
    line-height: 80rpx;
}

.twoM {
    position: absolute;
    top: 80rpx;
}

.nav {
    margin: 0 auto;
    height: 100rpx;
}

.navname {
    width: 160rpx;
    height: 100rpx;
    line-height: 100rpx;
    text-align: center;
    position: relative;
}

.hotelDown {
    width: 23rpx;
    height: 12rpx;
    position: absolute;
    top: 50%;
    transform: translateY(-50%);
    right: 20rpx;
}

.Exhibition_image {
    width: 100%;
    height: 344rpx;
}

.project view {
    outline: 1px solid #c9af98;
    color: #c9af98;
}