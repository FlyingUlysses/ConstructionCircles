.mewsSearch {
    display: flex;
    width: 90%;
    margin: 0 auto;
    height: 60rpx;
    background: #f2f3f4;
    justify-content: center;
    align-items: center;
    border-radius: 30rpx;
    position: relative;
}

.seaIcon {
    width: 24rpx;
    height: 24rpx;
    position: absolute;
    top: 50%;
    transform: translateY(-50%);
    left: 40%;
}

.mewsSearch .news_search_box,.mewsSearch input {
    font-size: 24rpx;
    line-height: 60rpx;
    color: #999;
    text-align: center;
    width: 100%;
}

@import "../../../__wuBaseWxss__/26.wxss";

.listMenu {
    border-radius: 15rpx;
}

.server_listImg {
    width: 140rpx;
    height: 140rpx;
    border-radius: 50%;
}

.server_button {
    width: 120rpx;
    height: 60rpx;
    line-height: 60rpx;
    border-radius: 28rpx;
    position: absolute;
    top: 50%;
    transform: translateY(-50%);
    right: 30rpx;
}

.server_md {
    position: fixed;
    right: 20rpx;
    bottom: 30rpx;
    z-index: 9999;
}

.server_md,.server_md image {
    width: 80rpx;
    height: 80rpx;
}