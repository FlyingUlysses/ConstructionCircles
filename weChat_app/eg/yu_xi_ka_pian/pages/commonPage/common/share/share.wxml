<view class="detailgg pos {{detailgg}}">
    <view class="detailgg1"></view>
    <view class="newsAlterBottom1 f28 c9 txt_center bgfff">分享到</view>
</view>
<view class="detailgg_share flex_around box_b pb30 plr30 {{detailgg}}">
    <view class="page pos">
        <view class="newsBottom1_sbtn">
            <button hoverClass="none" openType="share"></button>
        </view>
        <image src="https://www.laihukeji.com/wximg/news/wx1.png"></image>
        <view class="f24 c6 txt_center">微信好友</view>
    </view>
    <view bindtap="shareEvent" class="page" data-types="getHaibao">
        <image src="https://www.laihukeji.com/wximg/news/wx2.png"></image>
        <view class="f24 c6 txt_center">朋友圈海报</view>
    </view>
</view>
<view bindtap="shareEvent" class="tankuang {{shareAlter1}}" data-types="closehaibao"></view>
<view class="alter2 {{shareAlter1}}">
    <view class="alterTitle">
        <view bindtap="shareEvent" class="alter2Close" data-types="closehaibao">
            <image src="https://www.haojian.cn/wximg/close.png"></image>
        </view>
        <view class="txt_center c3 f34 bbe9s">朋友圈海报</view>
    </view>
    <view class="p30 box_b">
        <image bindtap="shareEvent" class="haibaoImg1" data-types="haibaoBigImg" data-url="{{haibaoImg}}" mode="scaleToFill" src="{{haibaoImg}}"></image>
    </view>
    <view class="f26 c3 txt_center">图片已保存至相册</view>
    <view class="f26 c3 txt_center">快去微信朋友圈分享吧~</view>
</view>
<view class="tankuang {{shareAlter}}"></view>
<view class="alter1 {{shareAlter}}">
    <view class="detailgg pos ptb30">
        <view class="detailgg1"></view>
        <view class="newsAlterBottom1 f28 c9 txt_center bgfff">分享到</view>
    </view>
    <view class="newsAlterBottom2 flex_between box_b">
        <view class="newsAlterBottom21 pos">
            <view class="newsBottom1_sbtn">
                <button hoverClass="none" openType="share"></button>
            </view>
            <image src="https://www.laihukeji.com/wximg/news/wx1.png"></image>
            <view class="f24 c6 txt_center">微信好友</view>
        </view>
        <view bindtap="shareEvent" class="newsAlterBottom21" data-types="getHaibao">
            <image src="https://www.laihukeji.com/wximg/news/wx2.png"></image>
            <view class="f24 c6 txt_center">朋友圈海报</view>
        </view>
    </view>
    <view class="newsAlterBottom3"></view>
    <view bindtap="shareEvent" class="newsAlterBottom4 txt_center f34 c3" data-types="closeShareAlter">取消</view>
</view>
