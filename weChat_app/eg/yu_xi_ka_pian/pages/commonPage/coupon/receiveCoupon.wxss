page {
    background: #f2f3f4;
}

.yhq_nav {
    position: fixed;
    top: 0;
    z-index: 99;
    width: 100%;
    height: 100rpx;
    line-height: 100rpx;
}

.yhq_nav>view {
    width: 50%;
}

.yhq_space {
    height: 120rpx;
}

.nav_line {
    background: #232d37;
    width: 60rpx;
    height: 4rpx;
    border-radius: 2rpx;
    position: absolute;
    display: none;
    bottom: 0;
    left: 50%;
    transform: translateX(-50%);
}

.active .nav_line {
    display: block;
}

.myCoupon1 {
    position: relative;
    overflow: hidden;
}

.myCoupon1,.myCoupon1Bg {
    width: 100%;
    height: 199rpx;
}

.myCoupon1_con {
    position: absolute;
    top: 0;
    left: 20rpx;
    height: 199rpx;
}

.f60 {
    font-size: 60rpx;
}

.myCoupon1_time {
    color: hsla(0,0%,100%,.5);
}

.myCoupon1_2 {
    position: absolute;
    width: 230rpx;
    top: 50%;
    transform: translateY(-50%);
    right: 0;
}

.myCoupon1_2_1 {
    width: 118rpx;
    height: 42rpx;
    border: 1rpx solid #ff6464;
    color: #ff6464;
    line-height: 42rpx;
    margin: 30rpx auto 0;
    border-radius: 22rpx;
}

.cffa {
    color: #ffafaf;
}

.nav {
    width: 100%;
    height: 100rpx;
    line-height: 100rpx;
    overflow: hidden;
}

.navLine {
    width: 60rpx;
    height: 4rpx;
    background: #232d37;
    position: absolute;
    bottom: 4rpx;
    left: 50%;
    margin-left: -30rpx;
    display: none;
}

.c3 .navLine {
    display: block;
}

.receiveCoupon {
    max-height: 700rpx;
    overflow-y: auto;
}

.receiveCoupon2 {
    position: absolute;
    right: 0;
    width: 230rpx;
    top: 50%;
    transform: translateY(-50%);
}

.receiveCoupon2_1,.receiveCoupon2_2 {
    font-size: 45rpx;
    color: #ff6464;
}

.receiveCoupon2_2 {
    font-weight: bolder;
}

.favorable_type {
    width: 60rpx;
    height: 28rpx;
    line-height: 28rpx;
    text-align: center;
    font-size: 24rpx;
    color: #ff5101;
    border: 1rpx solid #ff5101;
    border-radius: 4rpx;
    flex-shrink: 0;
    margin-right: 10rpx;
    margin-top: 7rpx;
}

.favorable_text {
    font-size: 28rpx;
    color: #666;
    line-height: 42rpx;
}

.effectiveTime {
    margin-left: 70rpx;
    margin-top: 20rpx;
}